{
    "name": "Demo",
    "instructions": "MCP Server - Model Context Protocol Server Implementation",
    "version": "1.0.0",
    "tools": {
        "add": {
            "name": "add",
            "description": "Add two integers together",
            "annotations": {
                "title": "Simple arithmetic add",
                "readOnlyHint": true,
                "destructiveHint": false,
                "idempotentHint": true,
                "openWorldHint": false
            }
        },
        "multiply": {
            "name": "multiply",
            "description": "Multiply two integers together",
            "annotations": {
                "title": "Simple arithmetic multiplication",
                "readOnlyHint": true,
                "destructiveHint": false,
                "idempotentHint": true,
                "openWorldHint": false
            }
        }
    },
    "resources": {
        "message": {
            "name": "message",
            "uri": "message://{name}",
            "description": "Generate a greeting message for a given name",
            "uriTemplate": "message://{name}"
        },
        "alive": {
            "name": "alive",
            "uri": "alive://",
            "description": "Return server status and server timestamp"
        }
    },
    "prompts": {
        "sme": {
            "name": "sme",
            "description": "Get a subject matter expert prompt for a specific topic",
            "arguments": {
                "properties": {
                    "topic": {
                        "type": "string",
                        "enum": [
                            "coding",
                            "math",
                            "writing"
                        ],
                        "description": "The topic for the prompt"
                    },
                    "subject": {
                        "type": "string",
                        "description": "The specific subject to explain"
                    }
                },
                "required": [
                    "topic",
                    "subject"
                ]
            }
        }
    },
    "serverInfo": {
        "name": "Demo",
        "version": "1.0.0"
    },
    "transport": {
        "type": "sse"
    }
}