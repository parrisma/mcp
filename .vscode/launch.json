      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/python/src/server:$PYTHONPATH"
      }
    },
    {
      "name": "React UI",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:5173/",
      "webRoot": "${workspaceFolder}/react/mcp-app/src"
    }
  ]
}
