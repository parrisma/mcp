{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Hello World MCP Server",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/python/src/server/mcp_server_runner.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "args": [
        "--config-dir=./config",
        "--config-file=hello_world_server_config.json",
        "--server-type=hello_world",
        "--port=6277",
        "--debug"
      ],
      "env": {
        "PYTHONPATH": "${workspaceFolder}/python/src/server:${workspaceFolder}/python/src/server/static_data_service:$PYTHONPATH"
      }
    },
    {
      "name": "Instrument Data MCP Server",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/python/src/server/mcp_server_runner.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "args": [
        "--config-dir=./config",
        "--config-file=instrument_server_config.json",
        "--server-type=instrument",
        "--server-data-path=./python/src/server/instrument_service",
        "--port=6278",
        "--debug"
      ],
      "env": {
        "PYTHONPATH": "${workspaceFolder}/python/src/server:$PYTHONPATH"
      }
    },
    {
      "name": "Client Data MCP Server",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/python/src/server/mcp_server_runner.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "args": [
        "--config-dir=./config",
        "--config-file=client_server_config.json",
        "--server-data-path=./python/src/server/client_service",
        "--server-type=client",
        "--port=6280",
        "--debug"
      ],
      "env": {
        "PYTHONPATH": "${workspaceFolder}/python/src/server:$PYTHONPATH"
      }
    },
    {
      "name": "Trade Data MCP Server",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/python/src/server/mcp_server_runner.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "args": [
        "--config-dir=./config",
        "--config-file=trade_server_config.json",
        "--server-data-path=./python/src/server/trade_service",
        "--server-type=trade",
        "--port=6281",
        "--debug"
      ],
      "env": {
        "PYTHONPATH": "${workspaceFolder}/python/src/server:$PYTHONPATH"
      }
    },
    {
      "name": "Static Data MCP Server",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/python/src/server/mcp_server_runner.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "args": [
        "--config-dir=./config",
        "--config-file=static_data_server_config.json",
        "--server-type=static_data",
        "--port=6279",
        "--debug"
      ],
      "env": {
        "PYTHONPATH": "${workspaceFolder}/python/src/server:$PYTHONPATH"
      }
    },
    {
      "name": "MCP Tests",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/python/tests/test_mcp_server.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/python/src/server:$PYTHONPATH",
        "MCP_LOCAL_SERVER_MODE": "False"
      }
    },
    {
      "name": "MCP Service Runner",
      "type": "debugpy",
      "request": "launch",
      "module": "python.src.client.mcp_client_runner",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "args": [
        "--host-list=${workspaceFolder}/python/src/client/mcp_host_list.json",
        "--ollama-host-url=http://ollama-gpu:11434",
        "--ollama-model=qwen2.5:72b"
      ],
      "env": {
        "PYTHONPATH": "${workspaceFolder}",
        "OPENROUTER_API_KEY": "${env:OPENROUTER_API_KEY}"
      }
    },
    {
      "name": "MCP Promethius",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/python/src/client/logtest.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/python/src/server:${workspaceFolder}"
      }
    },
    {
      "name": "MCP WEB",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/python/src/client/mcp_client_web_server.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}:${workspaceFolder}/python/src/server"
      }
    },
    {
