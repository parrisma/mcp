      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "Prompts Tests",
      "type": "debugpy",
      "request": "launch",
      "module": "python.src.client.prompts",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
